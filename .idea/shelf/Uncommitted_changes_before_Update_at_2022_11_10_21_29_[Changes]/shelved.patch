+package OracleClass;
+import oracle.jdbc.driver.OracleConnection;
+
+import java.sql.ResultSet;
+import java.sql.Statement;
+
+public class Adminbook {
+
+    private OracleConnection connect;
+    private Statement stmt;
+    public Adminbook(OracleConnection conn){
+        connect=conn;
+        try{
+            stmt=connect.createStatement();
+        }catch(Exception e){
+            System.out.println("Error");
+        }
+    }
+    public boolean hasbook(String id){
+        try {
+            ResultSet rs = stmt.executeQuery("SELECT BookName FROM BOOK WHERE BookID="+id);
+            if(rs.next()) {
+                return true;
+            }
+        }catch (Exception e) {
+            System.out.println("Error");
+        }
+        return false;
+    }
+    public String getname(String id){
+        try {
+            ResultSet rs = stmt.executeQuery("SELECT BookName FROM BOOK WHERE BookID=" + id);
+            if (rs.next()) {
+                return rs.getString(1);
+            }
+        }catch (Exception e) {
+            System.out.println("Error");
+        }
+        return "can not find the book";
+    }
+    public String getpub(String id){
+        try {
+            ResultSet rs = stmt.executeQuery("SELECT BookName FROM BOOK WHERE BookID=" + id);
+            if (rs.next()) {
+                return rs.getString(5);
+            }
+        }catch (Exception e) {
+            System.out.println("Error");
+        }
+        return "can not find the book";
+    }
+    public String getau(String id){
+        try {
+            ResultSet rs = stmt.executeQuery("SELECT BookName FROM BOOK WHERE BookID=" + id);
+            if (rs.next()) {
+                return rs.getString(3);
+            }
+        }catch (Exception e) {
+            System.out.println("Error");
+        }
+        return "can not find the book";
+    }
+
+}
